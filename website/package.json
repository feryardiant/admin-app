{
  "name": "@feryardiant/admin-web",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite-ssg build",
    "build:dev": "vite-ssg build --mode development",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "yarn lint --fix",
    "serve": "vite preview"
  },
  "types": "types.d.ts",
  "typings": "types.d.ts",
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "firebase": "^8.7",
    "js-cookie": "^2.2.1",
    "nprogress": "^0.2",
    "qs": "^6.10.1",
    "vue": "^3.0",
    "vue-router": "^4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2",
    "@vue/compiler-sfc": "^3.1",
    "@vue/server-renderer": "^3.1",
    "@vueuse/core": "^5.1.3",
    "@vueuse/head": "^0.5.0",
    "dotenv": "^10.0.0",
    "eslint-plugin-vue": "^7.13",
    "gray-matter": "^4.0.3",
    "markdown-it": "^12.1.0",
    "markdown-it-anchor": "^8.1.0",
    "markdown-it-link-attributes": "^3.0.0",
    "postcss": "^8.3",
    "postcss-nested": "^5.0",
    "vite": "^2.4",
    "vite-plugin-components": "^0.12.2",
    "vite-plugin-md": "^0.6.7",
    "vite-plugin-pages": "^0.15",
    "vite-plugin-pwa": "^0.8.1",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vite-plugin-windicss": "^1.2",
    "vite-ssg": "^0.11.4",
    "windicss": "^3.1",
    "workbox-window": "^6.1.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "eslintConfig": {
    "extends": "plugin:vue/vue3-recommended",
    "parser": "vue-eslint-parser",
    "ignorePatterns": [
      "dist/",
      "node_modules/",
      "public/",
      "**.old"
    ],
    "rules": {
      "vue/max-attributes-per-line": [
        "warn",
        {
          "singleline": 5
        }
      ],
      "vue/singleline-html-element-content-newline": "off",
      "vue/html-self-closing": "off",
      "vue/no-setup-props-destructure": "off",
      "vue/no-v-html": "off",
      "no-unused-vars": "off"
    }
  }
}
