{
  "name": "@feryardiant/admin-web",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite-ssg build",
    "build:dev": "vite-ssg build --mode development",
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "serve": "vite preview",
    "test": "web-test-runner --config tests/web-test-runner.config.mjs"
  },
  "types": "types.d.ts",
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "firebase": "^9.5.0",
    "js-cookie": "^3.0.1",
    "nprogress": "^0.2.0",
    "qs": "^6.10.1",
    "vue": "^3.2.22",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@feryardiant/eslint-config": "^0.0.5",
    "@types/markdown-it": "^12.2.3",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.22",
    "@vue/server-renderer": "^3.2.22",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vueuse/core": "^6.9.2",
    "@vueuse/head": "^0.5.1",
    "@web/test-runner": "^0.13.22",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.20.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-link-attributes": "^3.0.0",
    "postcss": "^8.3.11",
    "postcss-nested": "^5.0.6",
    "rollup": "^2.60.0",
    "unplugin-vue-components": "^0.15.6",
    "vite": "^2.6.14",
    "vite-plugin-md": "^0.11.4",
    "vite-plugin-pages": "^0.18.2",
    "vite-plugin-pwa": "^0.11.6",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-windicss": "^1.5.1",
    "vite-ssg": "^0.15.6",
    "vite-web-test-runner-plugin": "^0.1.0",
    "windicss": "^3.2.1",
    "workbox-window": "^6.4.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:vue/vue3-recommended",
      "@feryardiant"
    ],
    "ignorePatterns": [
      "dist/",
      "node_modules/",
      "public/",
      "*.old"
    ],
    "overrides": [
      {
        "files": [
          "*.vue"
        ],
        "parser": "vue-eslint-parser"
      }
    ],
    "rules": {
      "vue/max-attributes-per-line": [
        "warn",
        {
          "singleline": 5
        }
      ],
      "vue/singleline-html-element-content-newline": "off",
      "vue/html-self-closing": "off",
      "vue/no-setup-props-destructure": "off",
      "vue/no-v-html": "off",
      "no-undef": "off"
    }
  }
}
