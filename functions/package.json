{
  "name": "@feryardiant/firebase-functions",
  "version": "0.0.0",
  "author": {
    "name": "Fery Wardiyanto",
    "email": "ferywardiyanto@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src/*.js",
    "lint:fix": "eslint src/*.js --fix",
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "types.d.ts",
  "engines": {
    "node": "14"
  },
  "dependencies": {
    "@feryardiant/sendgrid-inbound-parser": "*",
    "@google-cloud/storage": "^5.8.5",
    "@rollup/plugin-json": "^4.1.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.10",
    "firebase-functions": "^3.14"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.6",
    "eslint": "^7.30",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "firebase-functions-test": "^0.3",
    "rollup": "^2.52",
    "rollup-plugin-terser": "^7.0.2"
  },
  "eslintConfig": {
    "extends": "standard",
    "ignorePatterns": [
      "node_modules/",
      "tests/",
      "**.old"
    ],
    "rules": {
      "no-unused-vars": 0,
      "one-var": 0
    }
  }
}
