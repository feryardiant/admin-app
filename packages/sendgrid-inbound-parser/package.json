{
  "name": "@feryardiant/sendgrid-inbound-parser",
  "version": "0.0.5",
  "description": "Simple library to parse SendGrid Inbound Mail",
  "author": "Fery Wardiyanto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/feryardiant/firebase-app.git",
    "directory": "packages/sendgrid-inbound-parser"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "README.md",
    "dist",
    "types.d.ts"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint *.js",
    "lint:fix": "eslint --fix *.js",
    "release": "standard-version",
    "test": "mocha",
    "watch": "rollup -c --watch"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "busboy": "^0.3.1",
    "mailparser": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "~7.14",
    "@babel/preset-env": "~7.14",
    "@babel/register": "~7.14",
    "@feryardiant/eslint-config-standard": "~0.0.2",
    "chai": "~4.3",
    "eslint": "~7.30",
    "express": "^4.17.1",
    "mocha": "~9.0",
    "prettier": "~2.3",
    "prettier-config-standard": "~4.0",
    "rollup": "~2.52",
    "sinon": "~11.1",
    "standard-version": "^9.0",
    "supertest": "^6.1.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@feryardiant/standard",
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "mocha": {
    "require": [
      "@babel/register"
    ],
    "reporter": "spec",
    "spec": [
      "index.spec.js"
    ],
    "ui": "bdd"
  },
  "prettier": "prettier-config-standard",
  "standard-version": {
    "skip": {
      "tag": true,
      "changelog": true
    }
  }
}
