{
  "name": "@feryardiant/sendgrid-inbound-parser",
  "version": "0.0.5",
  "description": "Simple library to parse SendGrid Inbound Mail",
  "author": "Fery Wardiyanto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/feryardiant/firebase-app.git",
    "directory": "packages/sendgrid-inbound-parser"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js"
    }
  },
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.cjs",
    "index.d.ts",
    "index.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "standard-version",
    "test": "mocha -r esm '*.spec.js'",
    "watch": "rollup -c --watch"
  },
  "dependencies": {
    "busboy": "^1.6.0",
    "mailparser": "^3.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-basic": "^0.25.2",
    "chai": "^4.3.6",
    "eslint": "^8.20.0",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "mocha": "^8.4.0",
    "rollup": "^2.77.1",
    "sinon": "^13.0.2",
    "standard-version": "^9.5.0",
    "supertest": "^6.2.4",
    "typescript": "^4.7.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "@antfu/basic"
    ],
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true,
      "changelog": true
    }
  }
}
