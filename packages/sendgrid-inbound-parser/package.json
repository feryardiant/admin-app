{
  "name": "@feryardiant/sendgrid-inbound-parser",
  "type": "module",
  "version": "0.0.5",
  "description": "Simple library to parse SendGrid Inbound Mail",
  "author": "Fery Wardiyanto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/feryardiant/firebase-app.git",
    "directory": "packages/sendgrid-inbound-parser"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "standard-version",
    "test": "mocha"
  },
  "dependencies": {
    "busboy": "^1.6.0",
    "mailparser": "^3.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.25.2",
    "@google-cloud/storage": "^5.20.5",
    "@types/busboy": "^1.5.0",
    "@types/mailparser": "^3.4.0",
    "@types/node": "16.x",
    "chai": "^4.3.6",
    "eslint": "^8.20.0",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "mocha": "^9.1.1",
    "rollup": "^2.77.1",
    "sinon": "^13.0.2",
    "standard-version": "^9.5.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": [
      "@antfu/ts"
    ],
    "overrides": [
      {
        "files": [
          "*.test.ts"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true,
      "changelog": true
    }
  }
}
