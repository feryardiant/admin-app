{
  "name": "@feryardiant/sendgrid-inbound-parser",
  "version": "0.0.5",
  "description": "Simple library to parse SendGrid Inbound Mail",
  "author": "Fery Wardiyanto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/feryardiant/firebase-app.git",
    "directory": "packages/sendgrid-inbound-parser"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "files": [
    "index.cjs",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "standard-version",
    "test": "mocha -r esm '*.spec.js'",
    "watch": "rollup -c --watch"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "busboy": "^1.4.0",
    "mailparser": "^3.4.0"
  },
  "devDependencies": {
    "@feryardiant/eslint-config": "^0.1.0",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "esm": "^3.2.25",
    "express": "^4.17.3",
    "mocha": "^8.4.0",
    "rollup": "^2.69.1",
    "sinon": "^13.0.1",
    "standard-version": "^9.3.2",
    "supertest": "^6.2.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "@feryardiant"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true,
      "changelog": true
    }
  }
}
