{
  "files.associations": {
    "*.css": "postcss"
  },
  // "volar.tsPlugin": true,
  "volar.tsPluginStatus": false,
  "json.schemas": [
    {
      "fileMatch": [
        ".github/workflows/*.yml"
      ],
      "url": "https://json.schemastore.org/github-workflow.json"
    },
    {
      "fileMatch": [
        "firebase.json"
      ],
      "url": "https://raw.githubusercontent.com/firebase/firebase-tools/master/schema/firebase-config.json"
    }
  ]
}
